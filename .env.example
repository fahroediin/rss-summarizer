# Supabase
SUPABASE_URL=your_supabase_url_here
SUPABASE_SERVICE_KEY=your_service_key_here

# Google Gemini API Keys for each agent
GEMINI_API_KEY_CATEGORIZER=your_categorizer_api_key_here
GEMINI_API_KEY_SUMMARIZER=your_summarizer_api_key_here

# Telegram API
TELEGRAM_BOT_TOKEN=your_bot_token_here

# RSS Feed URLs (pisahkan dengan koma, tanpa spasi setelah koma)
RSS_FEED_URLS=your_rss_feed_urls_here

# Cron Job Schedule (Format: Menit Jam Hari Bulan Hari Minggu)
# Contoh: '0 5 * * *' -> Setiap hari jam 5:00 pagi
CRON_SCHEDULE='0 5 * * *'

# Timezone for Cron Job (e.g., Asia/Jakarta, UTC, America/New_York)
CRON_TIMEZONE='Asia/Jakarta'